<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.yi.gradle_mybatis_c3p0.dao.ProductMapper">

	<select id="selectProductByAll" resultType="Product">
		select code, name from product
	</select>
	
	<select id="selectProductByCode" resultType="Product" parameterType="Product">
		select code, name
		from product
		where code = #{code}
	</select>
	

</mapper>